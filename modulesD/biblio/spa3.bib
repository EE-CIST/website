
@article{bahoken_approche_2016,
	title = {L'approche cartographique de la décomposition des matrices de flux},
	volume = {4},
	url = {https://hal.science/hal-01431365},
	abstract = {Indépendamment de sa complexité graphique intrinsèque (l'effet spaghetti), la cartographie directe des matrices de flux denses reÐt souvent un aspect compact, caractéristique d'un effet d'amalgame de l'information représentée. Cet effet résulte du mélange d'informations de nature différentes sur la même figure : celles qui portent sur la dynamique du système de flux et celles qui relèvent de sa stabilité. Pour y remédier, il convient de décomposer la matrice de flux, au préalable de sa cartographie.},
